{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "rewrites": [
    { "source": "/products", "destination": "/products" },
    { "source": "/review", "destination": "/review" },
    { "source": "/orders", "destination": "/orders" },
    { "source": "/users", "destination": "/users" }
  ]
}
