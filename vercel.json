{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "rewrites": [
    { "source": "/products", "destination": "/api/products" },
    { "source": "/review", "destination": "/api/review" },
    { "source": "/orders", "destination": "/api/orders" },
    { "source": "/users", "destination": "/api/users" }
  ]
}
